{
  "name": "gabrycina-ws",
  "version": "1.0.0",
  "description": "gabrycina website",
  "author": "",
  "license": "GPLv3",
  "scripts": {
    "start": "node index.js",
    "start-dev": "nodemon index.js",
    "clean": "rimraf views/{css/*,js/*,images/*}",
    "autoprefixer": "postcss -u autoprefixer -r views/css/*",
    "scss": "node-sass --output-style compressed -o views/css src/scss",
    "lint": "eslint src/js || true",
    "-scss": "style src/scss/*.scss --syntax scss || true",
    "imagemin": "imagemin src/images/* -o views/images",
    "serve": "browser-sync start --server --files \"views/css/*.css, views/js/*.js, **/*.html, !node_modules/**/*.html\"",
    "build:css": "run-s -scss scss autoprefixer",
    "build:js": "run-s",
    "build:images": "run-s imagemin",
    "build": "run-s build:*",
    "watch:css": "onchange \"src/scss\" -- run-s build:css",
    "watch:js": "onchange \"src/js\" -- run-s build:js",
    "watch:images": "onchange \"src/images\" -- run-s build:images",
    "watch": "run-p serve watch:*",
    "postinstall": "run-s build",
    "watchScss": "node-sass --watch src/scss -o views/css"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "autoprefixer": "^9.0.1",
    "babelify": "^10.0.0",
    "browser-sync": "^2.12.8",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "imagemin-cli": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "postcss-cli": "^6.0.0",
    "rimraf": "^2.5.4",
    "stylelint": "^9.4.0",
    "uglify-es": "^3.3.10"
  },
  "homepage": "https://gabrycina.it",
  "main": ".eslintrc.js",
  "dependencies": {
    "browserify": "^17.0.0",
    "create-index": "^2.6.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "global": "^4.4.0"
  }
}
